<Window x:Class="OE.NIK.ImPro.UI.Desktop.HistogramWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OE.NIK.ImPro.UI.Desktop"
        mc:Ignorable="d"
        DataContext="{Binding HistogramViewModel, Source={StaticResource Locator}}"
        Title="{Binding MessageFromParent}" Height="500" Width="500"
        WindowStartupLocation="CenterScreen"
        >
    <Grid>
        <TextBlock Text="{Binding MessageFromParent}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20"/>
    </Grid>
</Window>
